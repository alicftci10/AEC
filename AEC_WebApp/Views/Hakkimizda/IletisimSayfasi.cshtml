                </ul>
            </div>
        </div>
        <!-- /row -->
    </div>
    <!-- /container -->
</div>

<div class="container">
    <br />
    <div class="row text-center" style="color:black;">
        <h3>Adresimiz</h3>
        <br />
        <h5>@Model.Adres</h5>
    </div>
    <br />
    <br />
    <div class="row text-center" style="color:black;">
        <h3>Telefon Numaramız</h3>
        <br />
        <h5>@Model.Telefon</h5>
    </div>
    <br />
    <br />
    <div class="row text-center" style="color:black;">
        <h3>Email Adresimiz</h3>
        <br />
